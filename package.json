{
  "name": "@matheussn/sdk-generator",
  "version": "0.0.15",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/matheussn"
  },
  "dependencies": {
    "nunjucks": "^3.2.3",
    "prettier": "^2.7.1",
    "yaml": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@types/nunjucks": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "tsc -p . && yarn run copy:templates",
    "start": "ts-node src/index.ts",
    "lint": "eslint ./ --ext .ts --ext .js",
    "lint:fix": "eslint ./ --ext .ts --ext .js --fix",
    "types": "node src/bin/generate-types.js",
    "copy:templates": "cp -r ./src/templates/ ./dist/templates/"
  },
  "bin": {
    "sdk-gen": "dist/index.js"
  }
}
